generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Project {
    id        String @id @default(auto()) @map("_id") @db.ObjectId
    name      String
    url       String
    caniframe Boolean
    preview   String
    role      String
    startDate String
    status    String
    skillset  String[]
    description String
    collaborators String[]
    createdAt DateTime @default(now())
}

